{
  "name": "scaife-viewer",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf static/dist && mkdir -p static/dist/js && mkdir -p static/dist/css && mkdir -p static/dist/images",
    "build:dev": "npm run clean && webpack --config static/webpack.dev.js",
    "build:prod": "npm run clean && webpack --config static/webpack.prod.js",
    "dev": "npm run clean && concurrently --kill-others-on-fail \"python manage.py runserver\" \"webpack-dev-server --config static/webpack.dev.js\"",
    "heroku-postbuild": "npm run build:prod"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^4.1.1",
    "chai": "^4.1.2",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.10",
    "cssnano": "^3.10.0",
    "cssnano-cli": "^1.0.5",
    "document-register-element": "^1.8.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-vue": "^4.5.0",
    "exorcist": "^0.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "jsdom": "^11.10.0",
    "jsdom-global": "^3.0.2",
    "loaders.css": "^0.1.2",
    "lodash.debounce": "^4.0.8",
    "node-sass": "^4.9.0",
    "parse-link-header": "^1.0.1",
    "popper.js": "^1.14.3",
    "postcss-loader": "^2.1.4",
    "query-string": "^5.1.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "uglify-js": "^3.3.23",
    "vue": "^2.5.15",
    "vue-custom-element": "^2.1.0",
    "vue-loader": "^14.2.2",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.15",
    "vuex": "^3.0.0",
    "vuex-persistedstate": "^2.5.2",
    "vuex-router-sync": "^5.0.0",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-server": "^2.11.2",
    "webpack-merge": "^4.1.2"
  },
  "engines": {
    "node": "^8.6.0",
    "npm": "~5.7.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
